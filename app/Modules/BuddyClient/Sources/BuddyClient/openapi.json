{
    "openapi": "3.1.0",
    "info": {
        "title": "FastAPI",
        "version": "0.1.0"
    },
    "paths": {
        "/health/ping": {
            "get": {
                "summary": "Ping",
                "operationId": "ping_health_ping_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PingResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/app-api/v1/auth/register": {
            "post": {
                "summary": "Register",
                "operationId": "register_app_api_v1_auth_register_post",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "$ref": "#/components/schemas/Body_register_app_api_v1_auth_register_post"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Return whether the user account has been created successfully.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RegisterResponse"
                                }
                            }
                        }
                    },
                    "409": {
                        "description": "Provided user credentials already exist.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Invalid payload provided.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/app-api/v1/auth/login": {
            "post": {
                "summary": "Login",
                "operationId": "login_app_api_v1_auth_login_post",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "$ref": "#/components/schemas/Body_login_app_api_v1_auth_login_post"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Return the access token of the user logging in.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Invalid credentials provided.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Invalid payload provided.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/app-api/v1/auth/session": {
            "get": {
                "summary": "Session",
                "operationId": "session_app_api_v1_auth_session_get",
                "parameters": [
                    {
                        "name": "authorization",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Authorization"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Return the session data for the user that is logged in.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SessionResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Invalid credentials provided.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Invalid payload provided.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/app-api/v1/auth/refresh": {
            "post": {
                "summary": "Refresh",
                "operationId": "refresh_app_api_v1_auth_refresh_post",
                "parameters": [
                    {
                        "name": "authorization",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Authorization"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RefreshPayload"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Return the refresh token for the user that is logged in.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RefreshResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Invalid credentials provided.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Invalid payload provided.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/app-api/v1/llm/chats": {
            "get": {
                "summary": "List Chat Rooms",
                "operationId": "list_chat_rooms_app_api_v1_llm_chats_get",
                "parameters": [
                    {
                        "name": "authorization",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Authorization"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns the requesting users chat rooms",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ChatRoomListResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Resources requested while unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Create Chat Message",
                "operationId": "create_chat_message_app_api_v1_llm_chats_post",
                "parameters": [
                    {
                        "name": "authorization",
                        "in": "header",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Authorization"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CreateChatMessagePayload"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Return the chat response after sending a message.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CreateChatMessageResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Resources requested while unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden LLM has been selected",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Something unexpected went wrong",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Body_login_app_api_v1_auth_login_post": {
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "title": "Email"
                    },
                    "password": {
                        "type": "string",
                        "title": "Password"
                    }
                },
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "title": "Body_login_app_api_v1_auth_login_post"
            },
            "Body_register_app_api_v1_auth_register_post": {
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "title": "Email"
                    },
                    "password": {
                        "type": "string",
                        "title": "Password"
                    }
                },
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "title": "Body_register_app_api_v1_auth_register_post"
            },
            "BuddyErrorDetail": {
                "properties": {
                    "type": {
                        "type": "string",
                        "title": "Type"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Msg"
                    }
                },
                "type": "object",
                "required": [
                    "type",
                    "msg"
                ],
                "title": "BuddyErrorDetail"
            },
            "ChatRoomListItem": {
                "properties": {
                    "room_id": {
                        "type": "string",
                        "format": "uuid",
                        "title": "Room Id"
                    },
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "messages_count": {
                        "type": "integer",
                        "title": "Messages Count"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Updated At"
                    }
                },
                "type": "object",
                "required": [
                    "room_id",
                    "title",
                    "messages_count",
                    "created_at",
                    "updated_at"
                ],
                "title": "ChatRoomListItem"
            },
            "ChatRoomListResponse": {
                "properties": {
                    "detail": {
                        "type": "string",
                        "const": "OK",
                        "title": "Detail"
                    },
                    "data": {
                        "items": {
                            "$ref": "#/components/schemas/ChatRoomListItem"
                        },
                        "type": "array",
                        "title": "Data"
                    }
                },
                "type": "object",
                "required": [
                    "detail",
                    "data"
                ],
                "title": "ChatRoomListResponse"
            },
            "CreateChatMessagePayload": {
                "properties": {
                    "room_id": {
                        "anyOf": [
                            {
                                "type": "string",
                                "format": "uuid"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Room Id"
                    },
                    "llm_provider": {
                        "type": "string",
                        "minLength": 1,
                        "title": "Llm Provider"
                    },
                    "llm_key": {
                        "type": "string",
                        "minLength": 1,
                        "title": "Llm Key"
                    },
                    "message": {
                        "type": "string",
                        "minLength": 1,
                        "title": "Message"
                    }
                },
                "type": "object",
                "required": [
                    "llm_provider",
                    "llm_key",
                    "message"
                ],
                "title": "CreateChatMessagePayload"
            },
            "CreateChatMessageResponse": {
                "properties": {
                    "role": {
                        "type": "string",
                        "const": "assistant",
                        "title": "Role"
                    },
                    "content": {
                        "type": "string",
                        "title": "Content"
                    },
                    "detail": {
                        "type": "string",
                        "const": "Created",
                        "title": "Detail"
                    },
                    "room_id": {
                        "type": "string",
                        "format": "uuid",
                        "title": "Room Id"
                    },
                    "date": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Date"
                    }
                },
                "type": "object",
                "required": [
                    "role",
                    "content",
                    "detail",
                    "room_id",
                    "date"
                ],
                "title": "CreateChatMessageResponse"
            },
            "ErrorResponse": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/BuddyErrorDetail"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "required": [
                    "detail"
                ],
                "title": "ErrorResponse"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "LLMModel": {
                "properties": {
                    "provider": {
                        "type": "string",
                        "title": "Provider"
                    },
                    "key": {
                        "type": "string",
                        "title": "Key"
                    },
                    "display_name": {
                        "type": "string",
                        "title": "Display Name"
                    },
                    "description": {
                        "type": "string",
                        "title": "Description"
                    }
                },
                "type": "object",
                "required": [
                    "provider",
                    "key",
                    "display_name",
                    "description"
                ],
                "title": "LLMModel"
            },
            "LoginResponse": {
                "properties": {
                    "detail": {
                        "type": "string",
                        "const": "OK",
                        "title": "Detail"
                    },
                    "access_token": {
                        "type": "string",
                        "title": "Access Token"
                    },
                    "expiry_timestamp": {
                        "type": "integer",
                        "title": "Expiry Timestamp"
                    },
                    "token_type": {
                        "type": "string",
                        "const": "bearer",
                        "title": "Token Type"
                    },
                    "refresh_token": {
                        "type": "string",
                        "title": "Refresh Token"
                    }
                },
                "type": "object",
                "required": [
                    "detail",
                    "access_token",
                    "expiry_timestamp",
                    "token_type",
                    "refresh_token"
                ],
                "title": "LoginResponse"
            },
            "PingResponse": {
                "properties": {
                    "detail": {
                        "type": "string",
                        "const": "PONG",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "required": [
                    "detail"
                ],
                "title": "PingResponse"
            },
            "RefreshPayload": {
                "properties": {
                    "refresh_token": {
                        "type": "string",
                        "title": "Refresh Token"
                    }
                },
                "type": "object",
                "required": [
                    "refresh_token"
                ],
                "title": "RefreshPayload"
            },
            "RefreshResponse": {
                "properties": {
                    "detail": {
                        "type": "string",
                        "const": "OK",
                        "title": "Detail"
                    },
                    "access_token": {
                        "type": "string",
                        "title": "Access Token"
                    },
                    "expiry_timestamp": {
                        "type": "integer",
                        "title": "Expiry Timestamp"
                    },
                    "token_type": {
                        "type": "string",
                        "const": "bearer",
                        "title": "Token Type"
                    }
                },
                "type": "object",
                "required": [
                    "detail",
                    "access_token",
                    "expiry_timestamp",
                    "token_type"
                ],
                "title": "RefreshResponse"
            },
            "RegisterResponse": {
                "properties": {
                    "detail": {
                        "type": "string",
                        "const": "Created",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "required": [
                    "detail"
                ],
                "title": "RegisterResponse"
            },
            "SessionResponse": {
                "properties": {
                    "detail": {
                        "type": "string",
                        "const": "OK",
                        "title": "Detail"
                    },
                    "user": {
                        "$ref": "#/components/schemas/UserResponse"
                    },
                    "available_models": {
                        "items": {
                            "$ref": "#/components/schemas/LLMModel"
                        },
                        "type": "array",
                        "title": "Available Models"
                    }
                },
                "type": "object",
                "required": [
                    "detail",
                    "user",
                    "available_models"
                ],
                "title": "SessionResponse"
            },
            "UserResponse": {
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "title": "Email"
                    },
                    "tier": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Tier"
                    }
                },
                "type": "object",
                "required": [
                    "email",
                    "tier"
                ],
                "title": "UserResponse"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            }
        }
    }
}
